md-bottom-sheet.md-grid
    //md-card-content
        md-input-container
            label Query
            input(ng-model="query",data-ng-required="true", name="query", placeholder='Query', type="text", md-maxlength="30")
    md-list
        md-item(ng-repeat="item in items")
            md-button(ng-click="ItemClick($index)", class="md-grid-item-content")
                ng-md-icon.iconfill(icon="{{item.icon}}", size="46")
                .md-grid-text(ng-bind="item.name")